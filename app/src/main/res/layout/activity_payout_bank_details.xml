                        android:hint="@string/ssn"
                        android:maxLines="1"
                        android:paddingBottom="15dp"
                        android:paddingTop="20dp"
                        android:singleLine="true"
                        android:textColor="@color/text_shadow"
                        android:textColorHint="@color/app_green"
                        android:textSize="@dimen/midi"
